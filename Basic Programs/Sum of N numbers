# Approach-1 (while loop)

n = int(input())
sum=0
while n > 0:
    sum = sum + n
    n = n-1
print(sum)




# Approach-2 (for loop)

n = int(input())
i = 0
sum = 0
for i in range(n+1):
    sum = sum + i
    i = i + 1
print(sum)
 




# Approach-3 (by making a list)

n = int(input())
A = list(map(int, input().split()))
print(sum(A))
