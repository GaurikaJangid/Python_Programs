# Approach-1

num1 = input()
num2 = input()

print('num1 before swapping: ', num1)
print('num2 before swapping: ', num2)

temp = num1
num1 = num2
num2 = temp

print('num1 after swapping: ', num1)
print('num2 after swapping: ', num2)



# Approach-2 (by asigning)

num1 = input()
num2 = input()

print('num1 before swapping: ', num1)
print('num2 before swapping: ', num2)

num1, num2 = num2, num1

print('num1 after swapping: ', num1)
print('num2 after swapping: ', num2)
